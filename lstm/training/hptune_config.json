"hyperparameters": {
    "goal": "MAXIMIZE",
    "maxTrials": 40,
    "maxParallelTrials": 2,
    "params": [{
        "parameterName": "hidden_units",
        "type": "DISCRETE",
        "discreteValues": [64, 128, 256]
    },{
        "parameterName": "num_layers",
        "type": "DISCRETE",
        "discreteValues": [1, 2, 3]
    },{
        "parameterName": "learning_rate",
        "type": "DOUBLE",
        "minValue": 0.001,
        "maxValue": 0.1,
        "scaleType": "UNIT_LOG_SCALE"
    },{
        "parameterName": "decay_rate",
        "type": "DOUBLE",
        "minValue": 0.75,
        "maxValue": 0.95,
        "scaleType": "UNIT_LINEAR_SCALE"
    }]
}
